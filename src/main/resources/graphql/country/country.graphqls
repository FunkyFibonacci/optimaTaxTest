type Mutation {
    addCountry(input: CountryInput!): Country!
    addIncomeCategory(input: IncomeCategoryInput!): IncomeCategory!
}

type Country{
    id: ID
    createdAt:String
    updatedAt:String
    name:String
    code:String
}

input CountryInput{
    name:String!
    code:String!
}



type IncomeCategory {
    id: ID!
    name: String!
}

input IncomeCategoryInput {
    name: String!
}